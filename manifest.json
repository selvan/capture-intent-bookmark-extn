{
    "name": "hyper-search-extension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "This extention will push search intention & url to the indexer ",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "tabs"
    ]
}